{
  "trailingComma": "es5",
  "tabWidth": 4,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "singleAttributePerLine": false,
  "plugins": [
    "prettier-plugin-tailwindcss"
  ],
  "tailwindStylesheet": "./assets/css/tailwind.css",
  "printWidth": 160,
  "htmlWhitespaceSensitivity": "ignore",
  "vueIndentScriptAndStyle": true
}